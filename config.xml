<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hassan.foodpool" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Food Pool</name>
    <description>An awesome app.</description>
    <author email="hassan@appslab.io" href="http://ionicframework.com/">AppsLab</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="0" />
    <preference name="SplashScreenBackgroundColor" value="0xFFFFFFFF" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <plugin name="cordova-plugin-device" spec="~1.1.3" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.0" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.0" />
    <engine name="android" spec="~6.4.0" />
    <plugin name="cordova-plugin-facebook4" spec="^1.9.1">
        <variable name="APP_ID" value="2084422965110686" />
        <variable name="APP_NAME" value="foodpool" />
    </plugin>
    <allow-navigation href="http://192.168.10.3:8100" />
    <engine name="ios" spec="~4.4.0" />
    <allow-navigation href="http://169.254.144.103:8100" />
    <plugin name="cordova-plugin-advanced-http" spec="^1.9.1" />
    <allow-navigation href="http://192.168.10.6:8101" />
    <engine name="browser" spec="~5.0.3" />
</widget>
